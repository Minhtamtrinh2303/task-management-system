<div id="<%= dom_id task %>">
  <p>
    <strong>Title:</strong>
    <%= task.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= task.description %>
  </p>

  <p>
    <strong>Due date:</strong>
    <%= task.due_date %>
  </p>

  <p>
    <strong>Priority:</strong>
    <%= task.priority %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= task.status %>
  </p>

  <p>
    <strong>Completed date:</strong>
    <%= task.completed_date %>
  </p>

  <p>
    <strong>Reminder time:</strong>
    <%= task.reminder_time %>
  </p>

  <p>
    <strong>Project:</strong>
    <%= task.project_id %>
  </p>

  <p>
    <strong>Task status:</strong>
    <%= task.task_status_id %>
  </p>

</div>
